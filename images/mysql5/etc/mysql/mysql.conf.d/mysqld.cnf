[client]
socket = /var/run/sock/mysql5.sock

[mysqld_safe]
socket  = /var/run/sock/mysql5.sock
nice    = 0

[mysqld]
user                    = mysql
pid-file                = /var/run/sock/mysql5.pid
socket                  = /var/run/sock/mysql5.sock
port                    = 3306
basedir                 = /usr
datadir                 = /var/lib/mysql
tmpdir                  = /tmp
lc-messages-dir         = /usr/share/mysql
bind-address            = 127.0.0.1
skip-external-locking
skip-name-resolve
key_buffer_size         = 512M
max_allowed_packet      = 16M
thread_stack            = 192K
thread_cache_size       = 50
sql-mode                = STRICT_TRANS_TABLES,ERROR_FOR_DIVISION_BY_ZERO,NO_AUTO_CREATE_USER,NO_AUTO_VALUE_ON_ZERO,NO_ENGINE_SUBSTITUTION,NO_ZERO_DATE,NO_ZERO_IN_DATE
sysdate-is-now          = 1
myisam-recover-options  = BACKUP
max_connections         = 100
tmp-table-size          = 32M
max-heap-table-size     = 32M
query_cache_limit       = 1M
query_cache_size        = 16M
query-cache-type        = 0
query-cache-size        = 0
max-connect-errors      = 1000000
open-files-limit        = 65535
table-definition-cache  = 4096
table-open-cache        = 10
log_error               = /var/www/log/mysql5/error.log
slow-query-log          = 1
slow-query-log-file     = /var/www/log/mysql5/slow.log
long_query_time         = 2
symbolic-links          = 0
log-queries-not-using-indexes = 1
